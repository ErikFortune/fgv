{
  "name": "@fgv/ts-utils",
  "version": "4.0.2",
  "description": "Assorted Typescript Utilities",
  "main": "lib/index.js",
  "types": "dist/ts-utils.d.ts",
  "scripts": {
    "build": "heft build --clean",
    "clean": "heft clean",
    "test": "heft test --clean",
    "build-docs": "api-documenter markdown --input-folder ./temp --output-folder docs",
    "build-all": "rushx build; rushx build-docs",
    "test-handles": "jest --runInBand --detectOpenHandles",
    "clean-jest": "jest --clear-cache",
    "coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "fixlint": "eslint src --ext .ts --fix"
  },
  "sideEffects": false,
  "keywords": [
    "typescript",
    "json"
  ],
  "author": "Erik Fortune",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ErikFortune/fgv/issues"
  },
  "homepage": "https://github.com/ErikFortune/fgv/tree/main/libraries/ts-utils#readme",
  "devDependencies": {
    "@jest/expect-utils": "^29.7.0",
    "@microsoft/api-documenter": "^7.26.5",
    "@microsoft/api-extractor": "^7.49.1",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.2.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-matcher-utils": "^29.7.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "eslint-plugin-n": "^16.6.2",
    "jest-snapshot": "~29.7.0",
    "@rushstack/heft": "~0.68.13",
    "@rushstack/heft-node-rig": "~2.6.49",
    "@rushstack/eslint-config": "~3.7.0",
    "@types/heft-jest": "1.0.6",
    "@rushstack/heft-jest-plugin": "~0.14.3",
    "eslint-plugin-tsdoc": "~0.4.0"
  }
}
