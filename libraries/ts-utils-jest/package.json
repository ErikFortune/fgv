  "dependencies": {
    "@jest/expect-utils": "^29.7.0",
    "expect": "^29.7.0",
    "jest-matcher-utils": "^29.7.0",
    "jest-snapshot": "~29.7.0"
  },
  "devDependencies": {
    "@fgv/ts-utils": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
