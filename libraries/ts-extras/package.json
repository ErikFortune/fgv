{
  "name": "@fgv/ts-extras",
  "version": "2.0.1",
  "description": "Assorted Typescript Utilities",
  "main": "lib/index.js",
  "types": "dist/ts-extras.d.ts",
  "scripts": {
    "build": "heft test --clean",
    "clean": "heft clean",
    "test": "heft test",
