  "devDependencies": {
    "@jest/expect-utils": "^29.7.0",
    "@microsoft/api-documenter": "^7.23.17",
    "@microsoft/api-extractor": "^7.39.1",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^3.4.1",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.5",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
