    "@fgv/ts-res-browser": "workspace:*",
    "commander": "^11.0.0",
    "archiver": "~7.0.1",
    "@types/archiver": "~6.0.3"
  },
  "devDependencies": {
    "@fgv/ts-utils-jest": "workspace:*",
    "@rushstack/heft": "~0.74.0",
    "@rushstack/heft-node-rig": "~2.9.0",
    "@types/jest": "^29.5.14",
